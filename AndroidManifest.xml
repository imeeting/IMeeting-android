        <activity
            android:name=".account.AccountRegisterActivity"
            android:label=""
            android:screenOrientation="portrait"
            android:theme="@style/WithBgImg7NoTitleActivityTheme" >
        </activity>
        <activity
            android:name=".talkinggroup.TalkingGroupHistoryListActivity"
            android:label=""
            android:screenOrientation="portrait"
            android:theme="@style/WithBgImg7NoTitleActivityTheme" >
