<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.richitec.imeeting"
    android:versionCode="1"
    android:versionName="1.2.1" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="IMeetingAppLaunchActivity"
            android:theme="@style/AppActivityTheme" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".account.AccountSettingActivity"
            android:label=""
            android:theme="@style/WithBgImg7NoTitleActivityTheme" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".account.AccountRegisterActivity"
            android:label=""
            android:theme="@style/WithBgImg7NoTitleActivityTheme" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".talkinggroup.TalkingGroupHistoryListActivity"
            android:label=""
            android:theme="@style/WithBgImg7NoTitleActivityTheme" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".assistant.SettingActivity"
            android:label=""
            android:theme="@style/WithBgImg7NoTitleActivityTheme" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".contactselect.ContactSelectActivity"
            android:label="" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".assistant.SupportActivity"
            android:label="" android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".assistant.AboutActivity"
            android:label=""
            android:theme="@style/WithBgImg7NoTitleActivityTheme" android:screenOrientation="portrait">
        </activity>
        <activity android:name=".talkinggroup.TalkingGroupActivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Light.NoTitleBar"></activity>
        <activity android:name=".assistant.AccountChargeActivity" android:screenOrientation="portrait" android:theme="@style/WithBgImg7NoTitleActivityTheme"></activity>
    </application>

</manifest>